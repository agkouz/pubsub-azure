{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/static/*", "/*.js", "/*.css", "/*.png", "/*.jpg", "/*.gif", "/*.svg"]
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "globalHeaders": {
    "content-security-policy": "default-src 'self' https:; style-src 'self' 'unsafe-inline'; script-src 'self' 'unsafe-inline' 'unsafe-eval'"
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}
